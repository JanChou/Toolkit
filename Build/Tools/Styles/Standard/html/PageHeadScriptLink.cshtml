@model SharpDoc.TemplateContext
@using SharpDoc.Model
@helper Dump()
    {
    var syntax = "../syntaxhighlighter";
    <meta http-equiv="X-UA-Compatible" content="IE=9"/>
    <link href="../css/default.css" rel="stylesheet" type="text/css" />
    <link href='@syntax/styles/shCoreDefault.css' rel='stylesheet' type='text/css' />         
    <script src="../js/mootools.js" language="javascript"></script>
    <script src="../js/mootools-more.js" language="javascript"></script>
    <script src="../js/sharpdoc.js" language="javascript"></script>            
    <script src="@syntax/js/shCore.js" type="text/javascript"></script>
    <script src="@syntax/js/shBrushCpp.js" type="text/javascript"></script>
    <script src="@syntax/js/shBrushCSharp.js" type="text/javascript"></script>
    <script src="@syntax/js/shBrushJScript.js" type="text/javascript"></script>
    <script src="@syntax/js/shBrushVb.js" type="text/javascript"></script>
    <script type="text/javascript" language="javascript">
        SyntaxHighlighter.defaults['gutter'] = false;
        SyntaxHighlighter.defaults['smart-tabs'] = false
        SyntaxHighlighter.all();
    </script> 
}



